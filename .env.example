# Environment Variables for Contract Deployment
# Copy this file to .env and fill in your values

# Private key for deployment (NEVER commit this to version control)
# Format: 0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
PRIVATE_KEY=your_private_key_here

# RPC URLs for different networks
# Sepolia Testnet
SEPOLIA_URL=https://sepolia.infura.io/v3/your_project_id
# Or use Alchemy: https://eth-sepolia.g.alchemy.com/v2/your_api_key

# Mainnet (for production deployment)
MAINNET_URL=https://mainnet.infura.io/v3/your_project_id
# Or use Alchemy: https://eth-mainnet.g.alchemy.com/v2/your_api_key

# Etherscan API Key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas reporting (optional)
REPORT_GAS=true
